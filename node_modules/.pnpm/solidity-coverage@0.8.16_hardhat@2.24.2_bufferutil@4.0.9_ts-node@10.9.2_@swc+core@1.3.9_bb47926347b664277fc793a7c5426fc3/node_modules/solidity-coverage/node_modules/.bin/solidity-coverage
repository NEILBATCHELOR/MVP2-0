#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.2_bufferutil@4.0.9_ts-node@10.9.2_@swc+core@1.3.9_bb47926347b664277fc793a7c5426fc3/node_modules/solidity-coverage/plugins/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.2_bufferutil@4.0.9_ts-node@10.9.2_@swc+core@1.3.9_bb47926347b664277fc793a7c5426fc3/node_modules/solidity-coverage/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.2_bufferutil@4.0.9_ts-node@10.9.2_@swc+core@1.3.9_bb47926347b664277fc793a7c5426fc3/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.2_bufferutil@4.0.9_ts-node@10.9.2_@swc+core@1.3.9_bb47926347b664277fc793a7c5426fc3/node_modules/solidity-coverage/plugins/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.2_bufferutil@4.0.9_ts-node@10.9.2_@swc+core@1.3.9_bb47926347b664277fc793a7c5426fc3/node_modules/solidity-coverage/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.2_bufferutil@4.0.9_ts-node@10.9.2_@swc+core@1.3.9_bb47926347b664277fc793a7c5426fc3/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../plugins/bin.js" "$@"
else
  exec node  "$basedir/../../plugins/bin.js" "$@"
fi
