#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/vitest@3.2.3_@types+debug@4.1.12_@types+node@20.19.0_jsdom@26.1.0_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules/vitest/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/vitest@3.2.3_@types+debug@4.1.12_@types+node@20.19.0_jsdom@26.1.0_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/vitest@3.2.3_@types+debug@4.1.12_@types+node@20.19.0_jsdom@26.1.0_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules/vitest/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/vitest@3.2.3_@types+debug@4.1.12_@types+node@20.19.0_jsdom@26.1.0_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules:/Users/neilbatchelor/Cursor/Chain Capital Production/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitest@3.2.3_@types+debug@4.1.12_@types+node@20.19.0_jsdom@26.1.0_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vitest@3.2.3_@types+debug@4.1.12_@types+node@20.19.0_jsdom@26.1.0_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules/vitest/vitest.mjs" "$@"
fi
